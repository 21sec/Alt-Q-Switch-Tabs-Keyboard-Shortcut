{
"update_url":"http://clients2.google.com/service/update2/crx",

	"background": {
		"scripts": ["background.js"],
        "persistent": true
	},
    "options_page": "options.html",
	"name": "Toggle Switch Recent Last Tabs",
	"short_name": "TSRLT",
	"version": "1.6.4",
	"manifest_version": 2,
	"description": "Switch back and forth between your current and last recently used tab by pressing Alt + q on your keyboard.",
	"browser_action": {
		"default_title": "Toggle tabs",
		"default_icon": "icon.png"
	},
	"commands": {
		"toggle": {
			"suggested_key": {
				"default": "Alt+Q"
			},
			"description": "Toggle tabs"
		}
	},
	"permissions": [
		"tabs"
	],
	"icons": {
		"16": "icon_16.png",
		"32": "icon_32.png",
		"48": "icon_48.png",
		"128": "icon_48.png"
	}
}
